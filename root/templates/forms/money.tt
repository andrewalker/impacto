<input type="text" id="[% css_prefix _ form.name %]_[% field_name %]_input" class="[% css_prefix %]input [% css_prefix _ field_type %]_input [% css_prefix _ field_name %]_input" name="[% field_name %]" value="[% field.value %]" data-dojo-type="dijit.form.CurrencyTextBox"
data-dojo-props="
    required: true,
    constraints: { fractional: true },
    currency: 'R$',
    value: '[% field.value %]',
    invalidMessage: 'Valor incorreto. Digite os centavos.'
" />
